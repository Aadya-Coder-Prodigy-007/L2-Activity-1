day=int(input("what day where you born"))
print ("the day i was born on was", day)
month=int(input("what month where you born"))
print ("the month i was born on was", month)
year=int(input("what year where you born"))
print ("the year i was born on was", year)
time=int(input("what time where you born"))
print ("the time i was born on was", time)
gift=input("what was your best birthday gift")
print ("my best birthday gift was", gift)